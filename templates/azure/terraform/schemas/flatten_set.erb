<%  flatten_func_name = sdk_marshal.enqueue(property, $global_flatten_queue) -%>
<%  if output_var == 'd' -%>
if err := d.Set("<%= prop_name -%>", flatten<%= sdk_marshal.sdk_type.type_definition.build_ef_func_name(sdk_marshal, property)-%>(<%= input_var -%>)); err != nil {
    return fmt.Errorf("Error setting `<%= prop_name -%>`: %+v", err)
}
<%  else -%>
<%= output_var -%>["<%= prop_name -%>"] = flatten<%= sdk_marshal.sdk_type.type_definition.build_ef_func_name(sdk_marshal, property) -%>(<%= input_var -%>)
<%  end -%>